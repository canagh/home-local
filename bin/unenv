#!/usr/bin/env python3

import os
import sys

env = os.environ.items()
env = sorted(env, key=lambda e: (len(e[1]), - len(e[0])), reverse=True)
env = filter(lambda e: 3 <= len(e[1]), env)
env = map(lambda e: ('$'+e[0], e[1]), env)
env = list(env)

def main():
    for line in sys.stdin:
        for key,elem in env:
            line = line.replace(elem, key)
        print(line, end='')

if __name__ == '__main__':
    import argparse
    parser = argparse.ArgumentParser()
    args = parser.parse_args()
    main()
