#!/usr/bin/env python
def fib(n):
    a, b = 0, 1
    for i in range(n):
        a, b = b, a+b
    return a
assert fib(0) == 0
assert fib(1) == 1
assert fib(2) == 1
assert fib(3) == 2
assert fib(4) == 3
assert fib(5) == 5
import argparse
parser = argparse.ArgumentParser()
parser.add_argument('n', type=int, nargs='+', help='0-based')
args = parser.parse_args()
for n in args.n:
    print(str(fib(n)))
