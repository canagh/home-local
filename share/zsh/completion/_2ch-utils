#compdef 2ch-utils

typeset -A opt_args
local context state line

__2ch_utils_commands() {
    local -a commands
    commands=($(_call_program path 2ch-utils list))
    _values 'command' "${commands[@]}"
}

_arguments -s -S \
    '(- *)'{-h,--help}'[show help]' \
    '(- *)'{-l,--list}'[list commands]' \
    '*: :__2ch_utils_commands'
